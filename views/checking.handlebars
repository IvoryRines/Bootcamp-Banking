<h2>Total Checking</h2>

<section class="checking">
  <div class="checking-account">
    <container>
        <h3 class="balance">Available Balance: {{checking.account_balance}}</h3>
        <p>Account Details:</p>
          <ul>
            <li>Account: {{checking.account_number}}</li>
            <li>Last Transaction: </li>
          </ul>
    </container>
    <div class="deposit">
      <label>Deposit:</label>
      <input id="deposit-input"/>
    </div>
    <div class="withdraw">
      <label>Withdraw:</label>
      <input class="withdraw-input">
    </div>
  </div>
</section>