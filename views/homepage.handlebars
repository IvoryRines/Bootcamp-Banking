<button id="logout" type="click">Logout</button>
<section class="container">
  <img
    src="images/mobile-banking-image.jpg"
    alt="Mobile Banking"
    class="responsive-img"
  />
  <!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modal">
  Open an Account
</button>

<!-- Modal -->
<div class="modal fade" id="modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Select Account Type</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="checking">Checking</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="savings">Savings</button>
      </div>
    </div>
  </div>
</div>
  {{!-- <div class="account-links">
    {{> checking-preview}}
    {{> savings-preview}}
  </div> --}}
</section>

